{
  "expo": {
    "name": "App Fichaje Palacín",
    "slug": "app-fichaje-palacin",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/logoblanco.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoblanco.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3A8A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.palacin.fichaje"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoblanco.png",
        "backgroundColor": "#1E3A8A"
      },
      "package": "com.palacin.fichaje",
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logoblanco.png",
      "themeColor": "#1E3A8A",
      "backgroundColor": "#1E3A8A",
      "display": "standalone",
      "description": "Aplicación de fichaje para empleados de Palacín.",
      "name": "Fichaje Palacín"
    },
    "plugins": [
      "expo-build-properties",
      "expo-asset",
      "expo-font",
      "expo-updates"
    ],
    "extra": {
      "eas": {
        "projectId": "e76ddbc7-f593-4196-b452-f568c9635357"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/e76ddbc7-f593-4196-b452-f568c9635357",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}